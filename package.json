{
  "private": true,
  "name": "covid-19-response",
  "description": "This website provides a space for the global statistical community to share guidance, actions, tools and best practices to ensure the operational continuity of data programmes by National Statistical Offices, and to address issues of open and timely access to critical data needed by governments and all sectors of society to respond to the global COVID-19 crisis.",
  "version": "0.0.0",
  "author": "Thilo Maier <maier@un.org>",
  "homepage": "https://covid-19-response.unstatshub.org/",
  "scripts": {
    "dev": "cross-env GATSBY_TELEMETRY_DISABLED=1 gatsby develop",
    "build": "yarn run clean && cross-env GATSBY_TELEMETRY_DISABLED=1 gatsby build",
    "serve": "cross-env GATSBY_TELEMETRY_DISABLED=1 gatsby serve",
    "clean": "rm -rf ./public && cross-env GATSBY_TELEMETRY_DISABLED=1 gatsby clean",
    "inspect": "gatsby clean && node --inspect node_modules/.bin/gatsby develop",
    "lint": "yarn run lint:code && yarn run lint:other",
    "lint:code": "eslint . --ignore-path .gitignore",
    "lint:other": "yarn run prettier --check",
    "prettier": "prettier \"**/*.{md,mdx,css,yaml,yml}\" --ignore-path .gitignore",
    "format": "yarn run format:code && yarn run format:other",
    "format:code": "yarn run lint:code --fix",
    "format:other": "yarn run prettier --write",
    "purge": "rm -rf ./node_modules",
    "test": "yarn run lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@undataforum/components": "0.75.1",
    "@undataforum/gatsby-theme-base": "0.13.2",
    "@undataforum/gatsby-theme-blog": "0.11.4",
    "@undataforum/gatsby-theme-events": "0.8.3",
    "@undataforum/gatsby-theme-profiles": "0.8.2",
    "cross-env": "7.0.2",
    "deepmerge": "4.2.2",
    "gatsby": "2.21.19",
    "gatsby-image": "2.4.3",
    "gatsby-plugin-google-analytics": "2.3.1",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-intl": "4.5.3",
    "theme-ui": "0.3.1",
    "typeface-roboto": "0.0.75"
  },
  "devDependencies": {
    "@shopify/eslint-plugin": "36.0.2",
    "eslint": "6.8.0",
    "husky": "4.2.5",
    "lint-staged": "10.2.2",
    "prettier": "2.0.5"
  }
}
